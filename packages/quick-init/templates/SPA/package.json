{
  "name": "SPA",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dll": "cd client && rm -rf public/dist/*.dll.js && grunt webpack:buildDll",
    "dllDev": "cd client && rm -rf public/dist/*.dll.js && grunt webpack:devDll",
    "dev": "node ./scripts/client_i18n_build.js && cd client && grunt beforeDev && webpack --env.development",
    "build": "./scripts/client_build.sh",
    "transpile": "node ./scripts/client_i18n_build.js",
    "add_eslint": "cd scripts && node checkPreCommit.js",
    "eslint": "eslint *.js app/ lib/ config/ scripts/ client/**/ test/ --ext .js,.jsx --cache && cd client && grunt jsonlint",
    "merge_assets": "cd client && grunt merge_assets",
    "prepare": "cd scripts && node checkPreCommit.js"
  },
  "dependencies": {
    "antd": "^3.5.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.3.19",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "rsvp": "^3.1.0",
    "ufec": "^1.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.1.12",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "cssnano": "^3.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "grunt": "^1.0.1",
    "grunt-banner": "^0.6.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-replace": "^1.0.1",
    "grunt-webpack": "^2.0.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "less-var-parse": "^1.1.0",
    "load-grunt-tasks": "^3.3.0",
    "postcss-loader": "^1.3.2",
    "time-grunt": "^1.2.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^1.0.1",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.11.2",
    "webpack-dev-server": "^2.11.1"
  }
}
